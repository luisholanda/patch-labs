load("@rules_rust//crate_universe:defs.bzl", "crate", "crates_vendor")

crates_vendor(
    name = "crates_vendor",
    annotations = {
        "uuid": [crate.annotation(
            rustc_flags = [
                "--cfg",
                "uuid_unstable",
            ],
        )],
        "foundationdb": [crate.annotation(
            deps = [
                "//third-party/foundationdb:libfdb",
            ],
        )],
    },
    cargo_lockfile = "//:Cargo.Bazel.lock",
    manifests = ["//:Cargo.toml"],
    mode = "remote",
    tags = ["manual"],
    vendor_path = "crates",
)
